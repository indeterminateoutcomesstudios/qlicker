{
  "name": "qlicker",
  "private": true,
  "scripts": {
    "doc": "jsdoc imports/* docs/README.md -d docs/.jsdocs -t node_modules/docdash/",
    "start": "meteor --settings settings.json",
    "lint": "node_modules/.bin/standard \"imports/**/*.js*\" \"server/**/*.js*\" \"app-test/**/*.js*\"",
    "fix-lint": "node_modules/.bin/standard --fix \"imports/**/*.js*\" \"server/**/*.js*\" \"app-test/**/*.js*\"",
    "test:unit": "node_modules/.bin/spacejam test --once --driver-package practicalmeteor:mocha --settings settings.json",
    "test:app": "node_modules/.bin/spacejam test --once --full-app --driver-package practicalmeteor:mocha --settings settings.json",
    "test:unit-watch": "meteor test --driver-package practicalmeteor:mocha --settings settings.json",
    "test:app-watch": "meteor test --full-app --driver-package practicalmeteor:mocha --settings settings.json",
    "test:unit-coverage": "node_modules/.bin/spacejam test --driver-package practicalmeteor:mocha --port 4099 --coverage 'out_lcovonly out_coverage out_html' --xunit-out $CIRCLE_TEST_REPORTS/unit-results.xml",
    "test:app-coverage": "node_modules/.bin/spacejam test --full-app --driver-package practicalmeteor:mocha --port 4099 --coverage 'out_lcovonly out_coverage out_html in_coverage' --xunit-out $CIRCLE_TEST_REPORTS/full-app-results.xml",
    "test:unit-no-coverage": "node_modules/.bin/spacejam test --driver-package practicalmeteor:mocha --port 4099",
    "test:all": "npm run test:unit-watch && npm run test:app-watch"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "aws-sdk": "^2.289.0",
    "azure-storage": "^2.10.1",
    "babel-eslint": "^7.1.1",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.18.3",
    "docdash": "^0.4.0",
    "draft-js": "^0.10.0",
    "draft-js-export-html": "^0.5.2",
    "draftjs-to-html": "^0.6.0",
    "dropzone": "^4.3.0",
    "eslint": "^3.11.1",
    "fixed-data-table": "^0.6.5",
    "fixed-data-table-2": "^0.8.13",
    "immutable": "^3.8.1",
    "jquery": "^3.3.1",
    "jshint": "^2.9.6",
    "material-ui": "^0.20.2",
    "meteor-node-stubs": "~0.2.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "passport-saml": "^0.33.0",
    "rc-datetime-picker": "^1.6.1",
    "react": "^15.6.2",
    "react-addons-pure-render-mixin": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-csv": "^1.0.14",
    "react-dates": "^8.2.1",
    "react-datetime": "^2.16.3",
    "react-dom": "^15.6.2",
    "react-drag-sortable": "^1.0.6",
    "react-lines-ellipsis": "^0.7.3",
    "react-mounter": "^1.2.0",
    "react-select": "^1.3.0",
    "react-tools": "^0.13.3",
    "react-tooltip": "^3.6.1",
    "recharts": "^0.21.2",
    "semver": "^5.5.0",
    "streamsearch": "^0.1.2",
    "syntastic-react": "^1.0.3",
    "underscore": "^1.9.1",
    "uuid-1345": "^0.99.7",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "codecov": "^1.0.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.14.0",
    "eslint-plugin-react": "^6.9.0",
    "ignore-styles": "^5.0.1",
    "sinon": "^1.17.7",
    "spacejam": "https://github.com/serut/spacejam/tarball/windows-suppport-rc4",
    "standard": "^8.6.0"
  },
  "standard": {
    "globals": [
      "Router",
      "Meteor",
      "Accounts",
      "it",
      "describe",
      "confirm",
      "alertify"
    ]
  }
}
